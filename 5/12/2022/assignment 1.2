n=int(input("Enter how numbers your want "))
num=int(input("Enter any number ")) 
largest=num
smallest=num
i=1
while(i<n):
    num=int(input("Enter any number "))
    if(num>largest):
        largest=num 
    i=i+1 
if(num<smallest):
    smallest=num
print("The difference is: " , largest-smallest)

